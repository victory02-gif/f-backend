  <body>
    <div class="tracking-body">
      <div class="main-head">
        <div class="sub-head">
          <span class="head-img"
            ><img class="head-img0" src="f-logo.jpg"
          /></span>
          <span class="sing-in-text-box"
            ><h5 class="sing-in-text">Sign Up or Log in</h5></span
          >
          <span class="sign-in-i" data-page="signIn"
            ><i class="fa fa-user-circle"></i
          ></span>
          <span class="menu-bar"><i class="fas fa-bars"></i></span>
        </div>
        <form class="signIn-form" action="/login" method="get"></form>
      </div>
      <div class="menu-popup">
        <span class="options-tap o-t"
          ><h5 class="options-tap-text">Shipping</h5>
          <i class="fa fa-angle-down o-i-d"></i
        ></span>
        <span class="options-tap t-i-tap"
          ><h5 class="options-tap-text">Tracking</h5>
          <i class="fa fa-angle-down o-i-d"></i
        ></span>
        <span class="options-tap"
          ><h5 class="options-tap-text">Design & Print</h5>
          <i class="fa fa-angle-down o-i-d"></i
        ></span>
        <span class="options-tap"
          ><h5 class="options-tap-text">Locations</h5>
          <i class="fa fa-angle-down o-i-d"></i
        ></span>
        <span class="options-tap" data-support="support"
          ><h5 class="options-tap-text">Support</h5>
          <i class="fa fa-angle-down o-i-d"></i
        ></span>
        <span class="options-tap o-b"
          ><h5 class="options-tap-text">Shipping</h5>
          <i class="fa fa-angle-down o-i-d"></i
        ></span>
      </div>
      <div class="delivery-status">
        <div class="scheduled-details">
          <h2 class="scheduled-details-head-text">SCHEDULED DELIVERY DATE</h2>
        </div>
        <div class="scheduled-statues">
          <h1 class="scheduled-statues-text">Monday</h1>
          <h5 class="scheduled-statues-text-sub">2/01/2026 before 5:00PM</h5>
        </div>
        <div class="scheduled-delivery-status">
          <h5 class="scheduled-delivery-status-text">DELIVERY STATUS</h5>
          <div class="delivery-status0">
            <h5 class="delivery-status0-text">In Transit</h5>
            <span class="delivery-status0-i"
              ><i class="fa fa-circle-arrow-right d-a-i"></i
              ><i class="fa-solid fa-circle-info d-i"></i
            ></span>
          </div>
        </div>
      </div>
      <div class="delivery-messege-alert">
        <h5 class="messege-alert-head">Important Message!!</h5>
        <div class="messege-alert-content-box">
          <h5 class="messege-alert-text"></h5>
          <a class="managment-link" href="">fedex.com/payment</a>
        </div>
      </div>
      <div class="tracking-id-box">
        <h1 class="tracking-id-head-text">TRACKING ID</h1>
        <div class="tracking-id-text-box">
          <h5 class="tracking-id-text" data-id="<%= id %>"><%= id %></h5>
          <span class="tracking-id-star-i"><i class="fa fa-star"></i></span>
        </div>
      </div>
      <div class="tracking-body-info">
        <div class="tracking-animation-bar">
          <div class="tracking-animation-bar0"></div>
          <div class="indication-i">
            <i class="fa fa-circle-arrow-right i-i"></i
            ><i class="fa-solid fa-circle-info a-i"></i>
          </div>
        </div>
        <div class="tracking-details-info">
          <div class="tracking-box">
            <div class="from-body-box">
              <h5 class="from-body-head-text h-d">- FROM</h5>
              <div class="from-body-content-box c-b">
                <h5 class="from-body-from-text"></h5>
                <h5 class="from-body-head-creation-text">Label Created</h5>
                <h5 class="from-body-date-text"></h5>
              </div>
            </div>
            <div class="pReceved-box">
              <div class="pReceved-body-box">
                <h5 class="pReceved-body-head-text h-d">
                  - PACKAGE RECEIVED BY FEDEX
                </h5>
                <div class="pReceved-body-content-box c-b">
                  <h5 class="pReceved-body-pReceved-text"></h5>
                  <h5 class="pReceved-body-date-text"></h5>
                </div>
              </div>
            </div>
            <div class="intransit-box">
              <div class="intransit-body-box">
                <h5 class="intransit-body-head-text h-d">- IN TRANSIT</h5>
              </div>
            </div>
            <div class="customs-box">
              <div class="customs-body-box">
                <h5 class="customs-body-head-text h-d">
                  - UNDER CUSTOMS CLEARNCE
                </h5>
                <div class="customs-body-content-box c-b">
                  <h5 class="customs-body-location-text"></h5>
                  <h5 class="customs-body-date-text"></h5>
                </div>
              </div>
            </div>
            <div class="out-box">
              <div class="out-body-box">
                <h5 class="out-body-head-text h-d">- OUT FOR DELIVERY</h5>
              </div>
            </div>
            <div class="to-box">
              <div class="to-body-box">
                <h5 class="to-body-head-text h-d">- TO</h5>
                <div class="to-body-content-box c-b">
                  <h5 class="to-body-loctaion-text"></h5>
                  <h5 class="to-body-update-text h-d">UPDATED DELIVERY</h5>
                  <h5 class="to-body-pending-text">Pending</h5>
                  <h5 class="to-body-expected-date-text">Initially expected</h5>
                  <h5 class="to-body-date-text"></h5>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="update-email-box">
        <h5 class="update-email-text">
          ADD YOUR EMAIL TO STAY UPDATED ON THIS SHIPMENT
        </h5>
        <div class="update-email-box-content">
          <span class="update-email-in-box"
            ><input
              class="update-email-in"
              placeholder="YOUR EMAIL"
              type="text" /></span
          ><span class="email-submit-btw">SUBMIT</span>
        </div>
      </div>
      <div class="help-pop-up-box">
        <span class="help-pop-up-i" ><i class="fa fa-comment-dots"></i></span>
        <h5 class="help-pop-up-text" data-support="support">ASK FEDEX</h5>
      </div>
      <div class="delivery-details-tool-bar-box">
        <div class="shipment-facts d-t">
          <h5 class="shipment-facts-text">Shipment facts</h5>
          <i class="fa fa-angle-down s-i-d d-ar-i"></i>
          <i class="fa fa-angle-up s-i-u d-ar-i"></i>
        </div>
        <div class="shipment-facts-content">
          <div class="shipment-facts-overview-box m-b">
            <div class="overview-box">
